{
    "require-dev": {
        "phpspec/phpspec": "~2.2",
        "behat/behat": "~3.0",
        "bossa/phpspec2-expect": "~1.0",
        "symfony/process": "~2.7"
    },
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "symfony/filesystem": "~2.7"
    },
    "autoload": {
      "psr-4": {
        "Mdb\\PhpSpecRenameExtension\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Foo\\": "_test/src",
        "spec\\Foo\\": "_test/spec"
      }
    }
}
